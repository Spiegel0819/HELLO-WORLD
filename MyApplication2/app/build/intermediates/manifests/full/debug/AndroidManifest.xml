<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jacob_ke.myapplication"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/robot"
        android:label="@string/app_name" >
        <activity
            android:name="com.example.jacob_ke.myapplication.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.jacob_ke.myapplication.DisplayMessageActivity_1"
            android:label="@string/title_activity_display_main"
            android:supportsRtl="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
        </activity>
        <activity
            android:name="com.example.jacob_ke.myapplication.DisplayMessageActivity_2"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.example.jacob_ke.myapplication.DisplayMessageActivity_1"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DisplayMessageActivity_1" />
        </activity>
    </application>

</manifest>